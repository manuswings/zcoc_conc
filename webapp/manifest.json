{
  "_version": "1.48.0",
  "sap.app": {
    "id": "lam.zcoc_conc",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.11.1",
      "toolsId": "705bec5b-03ec-4770-bd80-b890cc59ed7d"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZCOC_CONC_SRV/",
        "type": "OData",
        "settings": {
          "defaultBindingMode": "TwoWay",
          "annotations": [
            "zcoc_conc"
          ],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "zcoc_conc": {
        "uri": "annotation/zcoc_conc.xml",
        "type": "ODataAnnotation",
        "settings": {
          "maxAge": 0,
          "localUri": "annotation/zcoc_conc.xml"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "ZCOC_CONC-display": {
          "semanticObject": "ZCOC_CONC",
          "action": "display",
          "title": "{{flpTitle}}",
          "subTitle": "{{flpSubtitle}}",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.108.6",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "lam.zcoc_conc.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "lam.zcoc_conc.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteHome",
          "pattern": ":?query:",
          "target": [
            "TargetHome"
          ]
        },
        {
          "name": "RouteCreate",
          "pattern": "Create",
          "target": [
            "TargetCreate"
          ]
        }        
      ],
      "targets": {
        "TargetHome": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Home",
          "viewName": "Home"
        },
        "TargetCreate": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Create",
          "viewName": "Create"
        }        
      }
    },
    "rootView": {
      "viewName": "lam.zcoc_conc.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}